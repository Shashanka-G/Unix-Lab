else
  big=$n2
  small=$n1
fi

while [ $small -gt 0 ];
do
  rem=$(($big % $small))
  big=$small
  small=$rem
done

echo "GCD : $big"
echo "LCM : $(($(($n1 * $n2)) / $big))"
